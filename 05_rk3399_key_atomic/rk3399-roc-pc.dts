		user {
			label = "firefly:yellow:user";
			linux,default-trigger = "ir-user-click";
			default-state = "off";
			gpios = <&gpio0 2 GPIO_ACTIVE_HIGH>;
			pinctrl-names = "default";
		    pinctrl-0 = <&led_user>;	
