*/
};

		my_led {
			compatible = "firefly,myled";
			default-state = "off";
			gpios = <&gpio0 2 GPIO_ACTIVE_HIGH>;
			pinctrl-names = "default";
		    pinctrl-0 = <&led_user>;	
		};

		

		my_key {
			compatible = "firefly,mykey";
			default-state = "off";
			gpios = <&gpio0 5 IRQ_TYPE_EDGE_RISING>;
			
			pinctrl-names = "default";
		    pinctrl-0 = <&pwrbtn>;	
		};

